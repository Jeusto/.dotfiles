[
  {
    "name": "No_scripts",
    "description": "Select to edit scripts",
    "icon": null,
    "script": "xdg-open",
    "default_arguments": ["~/.config/ulauncher/scripts.json"]
  },
  {
    "name": "English keyboard",
    "icon": "~/.config/ulauncher/icons/en.png",
    "script": "setxkbmap us"
  },
  {
    "name": "French keyboard",
    "icon": "~/.config/ulauncher/icons/fr.png",
    "script": "setxkbmap fr"
  },
  {
    "name": "Audio: pause/play",
    "icon": "~/.config/ulauncher/icons/playpause.png",
    "script": "xdotool key XF86AudioPlay"
  },
  {
    "name": "Audio: previous",
    "icon": "~/.config/ulauncher/icons/previous.png",
    "script": "xdotool key XF86AudioPrev"
  },
  {
    "name": "Audio: next",
    "icon": "~/.config/ulauncher/icons/next.png",
    "script": "xdotool key XF86AudioNext"
  },
  {
    "name": "Yeelight: toggle",
    "icon": "~/.config/ulauncher/icons/toggle.png",
    "script": "yeelight-cli 192.168.1.20 toggle"
  },
  {
    "name": "Yeelight: default (preset)",
    "icon": "~/.config/ulauncher/icons/day.png",
    "script": "yeelight-cli 192.168.1.20 preset reading"
  },
  {
    "name": "Yeelight: cosy home (preset)",
    "icon": "~/.config/ulauncher/icons/cosyhome.png",
    "script": "yeelight-cli 192.168.1.20 preset cosyhome"
  },
  {
    "name": "Yeelight: night (preset)",
    "icon": "~/.config/ulauncher/icons/night.png",
    "script": "yeelight-cli 192.168.1.20 preset night"
  }
]
